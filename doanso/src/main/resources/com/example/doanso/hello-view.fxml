<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.doanso.HelloController">

  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>
  <HBox alignment="CENTER">
    <Label text="Random Number"/>
    <TextField fx:id="guessField" layoutX="159.0" layoutY="84.0" />
    <Button layoutX="173.0" layoutY="133.0" mnemonicParsing="false" onAction="#checkGuess" text="Guess" />
    <Label fx:id="resultLabel" layoutX="144.0" layoutY="185.0" prefHeight="17.0" prefWidth="112.0" />
  </HBox>

</VBox>
